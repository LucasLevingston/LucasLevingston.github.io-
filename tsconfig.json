{
	"compilerOptions": {
		"jsx": "react",

		"target": "es2016" ,

		"module": "commonjs",
		"resolveJsonModule": true,                       
		"esModuleInterop": true ,
		"forceConsistentCasingInFileNames": true, 
		"strict": true ,
		"skipLibCheck": true, 
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    }
	},
	"include": ["src", "public", "typings"]
}
